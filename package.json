{
	"config": {
		"commitizen": {
			"path": "node_modules/cz-git"
		}
	},
	"dependencies": {
		"@astrojs/cloudflare": "^12.6.7",
		"@astrojs/mdx": "^4.3.4",
		"@astrojs/rss": "^4.0.12",
		"@astrojs/sitemap": "^3.5.1",
		"@astrojs/vue": "^5.1.0",
		"astro": "^5.13.5",
		"sharp": "^0.34.2",
		"vue": "^3.5.21"
	},
	"devDependencies": {
		"@antfu/eslint-config": "^5.2.2",
		"@biomejs/biome": "2.2.4",
		"@iconify-json/carbon": "^1.2.13",
		"@iconify-json/logos": "^1.2.9",
		"@iconify-json/mdi": "^1.2.3",
		"@prettier/plugin-xml": "^3.4.2",
		"@types/bun": "^1.2.21",
		"@types/node": "^24.3.1",
		"@unocss/eslint-plugin": "^66.5.0",
		"@unocss/reset": "^66.5.0",
		"eslint": "^9.35.0",
		"eslint-plugin-astro": "^1.3.1",
		"eslint-plugin-format": "^1.0.1",
		"lefthook": "^1.12.4",
		"prettier": "^3.6.2",
		"prettier-plugin-astro": "^0.14.1",
		"rimraf": "^6.0.1",
		"unocss": "^66.5.0",
		"wrangler": "^4.34.0"
	},
	"name": "coderhangb",
	"scripts": {
		"astro": "astro",
		"build": "astro build",
		"cf-typegen": "wrangler types",
		"commit": "git-cz",
		"deploy": "astro build && wrangler pages deploy",
		"dev": "astro dev",
		"lint": "eslint",
		"lint:fix": "eslint --fix",
		"lint:js": "biome lint .",
		"lint:js:fix": "biome lint --write --unsafe .",
		"prepare": "lefthook install",
		"preview": "astro build && wrangler pages dev",
		"prune": "rimraf --glob 'node_modules' 'bun.lock' '.astro' 'dist'"
	},
	"type": "module",
	"validate-branch-name": {
		"errorMsg": "Brand name is invalid, please rename your current branch",
		"pattern": "^(master|main|develop){1}$|^(chore|docs|feat|fix|hotfix|perf|refactor|release|test)/.+$"
	},
	"version": "0.0.1"
}
